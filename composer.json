{
    "name": "sbshara/breadcrumbs",
    "description": "a ready out of the box breadcrumbs partial to work with laravel framework",
    "keywords": [
        "laravel", 
        "breadcrumbs", 
        "URL slice", 
        "segments",
        "bootstrap",
        "package",
        "library",
        "ready",
        "easy"
    ],
    
    "license": "MIT",
    
    "authors": [
      {
        "name": "Shiblie Bshara",
        "email": "shiblie@bshara.com"
      }
    ],
    
    "autoload": {
      "psr-4": {
        "sbshara\\breadcrumbs\\": "src/"
      }
    },
    
    "autoload-dev": {
      "classmap": [
        "tests/test.php"
      ]
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "sbshara\\breadcrumbs\\ServiceProvider"
            ]
        }
    },
    
    "require": {
      "laravel/framework": "~5.6.0|~5.7.0",
      "php": ">=7.0"
    },
    
    "require-dev": {
      "phpunit/phpunit": "~4.0"
    },

    "suggest": {
      "sbshara/laraadminlte": "ready AdminLTE template to use with Laravel Framework"
    }
  }
